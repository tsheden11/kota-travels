{
  "name": "eleventy-decap-site",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all2 --parallel start:eleventy start:tailwind",
    "start:eleventy": "eleventy --serve",
    "start:tailwind": "postcss src/css/style.css -o _site/css/style.css --watch",
    "build:tailwind": "tailwindcss -i src/css/style.css -o _site/css/style.css --minify",
    "build:eleventy": "eleventy",
    "dev": "npm run start",
    "build": "npm-run-all build:tailwind build:eleventy"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "autoprefixer": "^10.4.24",
    "npm-run-all": "^1.1.3",
    "npm-run-all2": "^8.0.4",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "postcss-import": "^16.1.1",
    "tailwindcss": "^3.4.0"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.19"
  }
}
